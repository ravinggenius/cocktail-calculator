<div class="loading-spinner"></div>

<div class="techniques-list techniques-wrapper" id="techniqueList" data-controller="BlogLayout">

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Dilution</th>
            <th>Volume</th>
            <th>Ethanol</th>
            <th>Sugar</th>
            <th>Acid</th>
        </tr>
    </thead>

    <tbody>
        {.repeated section items}
            <tr>
                <td>{title}</td>
                <td>{customContent.dilutionLow} - {customContent.dilutionHigh}</td>
                <td>{customContent.volumeLow} - {customContent.volumeHigh}</td>
                <td>{customContent.ethanolLow} - {customContent.ethanolHigh}</td>
                <td>{customContent.sugarLow} - {customContent.sugarHigh}</td>
                <td>{customContent.acidLow} - {customContent.acidHigh}</td>
            </tr>
        {.end}
    </tbody>
</table>

</div>
